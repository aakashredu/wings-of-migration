    "Flight_Duration_hours": {
      "type": "number",
      "description": "Flight Duration Hours in hours",
      "example": "49.5"
    },
    "Average_Speed_kmph": {
      "type": "number",
      "description": "Average Speed Kmph in kilometers per hour",
      "example": "47.82"
    },
    "Max_Altitude_m": {
      "type": "number",
      "description": "Max Altitude M in meters",
      "example": "5280"
    },
    "Min_Altitude_m": {
      "type": "number",
      "description": "Min Altitude M in meters",
      "example": "285"
    },
    "Temperature_C": {
      "type": "number",
      "description": "Temperature C in Celsius",
      "example": "-2.2"
