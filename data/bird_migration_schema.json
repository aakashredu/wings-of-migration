    "Wind_Speed_kmph": {
      "type": "number",
      "description": "Wind Speed Kmph in kilometers per hour",
      "example": "9.1"
    },
    "Humidity_%": {
      "type": "number",
      "description": "Humidity % in percentage",
      "example": "43"
