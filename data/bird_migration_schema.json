    "Bird_ID": {
      "type": "string",
      "description": "Bird Id",
      "example": "B1000"
    },
    "Species": {
      "type": "string",
      "description": "Species",
      "example": "Warbler"
    },
    "Region": {
      "type": "string",
      "description": "Region",
      "example": "South America"
    },
    "Habitat": {
      "type": "string",
      "description": "Habitat",
      "example": "Grassland"
    },
    "Weather_Condition": {
      "type": "string",
      "description": "Weather Condition",
      "example": "Stormy"
    },
    "Migration_Reason": {
      "type": "string",
      "description": "Migration Reason",
      "example": "Feeding"
    },
    "Start_Latitude": {
      "type": "number",
      "description": "Start Latitude",
      "example": "11.9065664413375"
    },
    "Start_Longitude": {
      "type": "number",
      "description": "Start Longitude",
      "example": "-169.378250688302"
    },
    "End_Latitude": {
      "type": "number",
      "description": "End Latitude",
      "example": "30.3776466643031"
    },
    "End_Longitude": {
      "type": "number",
      "description": "End Longitude",
      "example": "-21.366879258392"
    },
    "Flight_Distance_km": {
      "type": "number",
      "description": "Flight Distance Km in kilometers",
      "example": "1753.79"
