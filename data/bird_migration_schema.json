      "type": "string",
      "description": "Migration Success",
      "example": "Failed"
    },
    "Recovery_Location_Known": {
      "type": "string",
      "description": "Recovery Location Known",
      "example": "No"
    },
    "Recovery_Time_days": {
      "type": "number",
      "description": "Recovery Time Days in days",
      "example": "102"
    },
    "Observation_Counts": {
      "type": "number",
      "description": "Observation Counts",
      "example": "56"
    },
    "Observation_Quality": {
      "type": "string",
      "description": "Observation Quality",
      "example": "Low"
    },
    "Origin": {
      "type": "string",
      "description": "Origin",
      "example": "11.906566441337574, -169.37825068830264"
    },
    "Destination": {
      "type": "string",
      "description": "Destination",
      "example": "30.377646664303118, -21.36687925839209"
    }
